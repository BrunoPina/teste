<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" >

	<b:Row>
		<b:Column addStyleNames="rowcolumn-vertically-centered" size="XS_5">
			<b:Image addStyleNames="center-block img-card-contato" ui:field="imgContato" type="CIRCLE" />
		</b:Column>
		<b:Column addStyleNames="rowcolumn-vertically-centered" size="XS_7">
			<b:Heading size="H4" addStyleNames="lbl-top" ui:field="hdNome"/>
			<b:Heading size="H5" addStyleNames="lbl-center" ui:field="hdCargo"/>
		</b:Column>
		<b:Column addStyleNames="rowcolumn-vertically-centered" size="XS_12">
			<b:Tooltip ui:field="tooltipEmail">
				<b:Heading size="H5" addStyleNames="lbl-bottom" ui:field="hdEmail"/>
			</b:Tooltip>
		</b:Column>
		<b:Label addStyleNames="card-label" ui:field="labelAtivoInativo" text="Ativo" type="SUCCESS" />
	</b:Row>
	
</ui:UiBinder>