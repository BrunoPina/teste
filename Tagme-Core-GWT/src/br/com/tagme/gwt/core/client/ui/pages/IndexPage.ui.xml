<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<ui:with field="res" type="br.com.tagme.gwt.theme.sankhya.client.CommonImagesFactory"/>
	
	<b:Row height="100%">
	
		<b:Column size="XS_12" height="100%">
		
			<b:Row>
				<b:Column hiddenOn="XS" size="XS_2" offset="XS_2" addStyleNames="rowcolumn-vertically-centered">
					<b:Image addStyleNames="center-block doubleline-blockquote-image" resource="{res.create.oscar}" type="CIRCLE"/>
				</b:Column>
				
				<b:Column hiddenOn="XS" size="XS_6" addStyleNames="rowcolumn-vertically-centered">
					<b:BlockQuote addStyleNames="blockquote-reverse center-block doubleline-blockquote">
						<h:Paragraph text="&quot;Estamos satisfeitos com a flexibilidade do sistema e com o atendimento personalizado. A ABAD recomenda a Sankhya.&quot;" />
						<h:Small text="Oscar Attisano, Superintendente Executivo da ABAD" />
					</b:BlockQuote>
				</b:Column>
			</b:Row>
			
			<b:Row>
				<b:Column ui:field="thumbPos1" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos2" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos3" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos4" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos5" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos6" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos7" size="XS_12 SM_6 MD_4 LG_3" />
				
				<b:Column ui:field="thumbPos8" size="XS_12 SM_6 MD_4 LG_3" />
			</b:Row>
			
		</b:Column>
		
	</b:Row>
	
</ui:UiBinder> 