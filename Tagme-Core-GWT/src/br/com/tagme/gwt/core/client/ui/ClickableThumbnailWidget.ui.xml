<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<b:ThumbnailLink ui:field="thumbnailLink" addStyleNames="thumbnail-link">
		<b:Tooltip ui:field="tooltip">
			<b:ThumbnailPanel addStyleNames="center-block iconed-thumbnail">
				<b:Icon type="USD" ui:field="icon" size="TIMES2" addStyleNames="thumbnail-icon center-block thumbnail-icon"/>
				<b:Caption>
					<b:Heading ui:field="heading" size="H4"/>
					<h:Paragraph ui:field="paragraph"/>
				</b:Caption>
			</b:ThumbnailPanel>
		</b:Tooltip>
	</b:ThumbnailLink>
	
</ui:UiBinder> 