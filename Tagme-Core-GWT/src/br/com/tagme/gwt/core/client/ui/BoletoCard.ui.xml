<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:c="urn:import:org.gwtbootstrap3.extras.card.client.ui">

	<b:Row>
		<b:Column size="XS_12">
			<b:Icon type="CALENDAR" size="TIMES2" addStyleNames="center-block card-icon" ui:field="iconBoleto" />
		</b:Column>
		<b:Column size="XS_12">
			<b:Heading ui:field="lblTop" size="H4" addStyleNames="lbl-top" />
		</b:Column>
		<b:Column size="XS_12">
			<b:Tooltip ui:field="tooltipNatureza">
				<b:Heading ui:field="lblCenter" size="H5" addStyleNames="lbl-center"/>
			</b:Tooltip>
		</b:Column>
		<b:Column size="XS_12">
			<b:Heading ui:field="lblBottom" size="H5" addStyleNames="lbl-bottom" />
		</b:Column>
		<b:Label addStyleNames="card-label" ui:field="labelStatus" />
	</b:Row>
	
</ui:UiBinder>