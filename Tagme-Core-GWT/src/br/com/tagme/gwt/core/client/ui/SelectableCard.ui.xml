<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:c="urn:import:org.gwtbootstrap3.extras.card.client.ui">
	
	<b:Column size="XS_12" ui:field="column">
		<c:Card ui:field="card">
			<c:Front ui:field="cardFront">
				<b:Icon ui:field="selectedIconFront" type="CHECK" visible="false" />
			</c:Front>
			<c:Back ui:field="cardBack">
				<b:Icon ui:field="selectedIconBack" type="CHECK" visible="false" />
			</c:Back>
		</c:Card>
	</b:Column>
	
</ui:UiBinder> 