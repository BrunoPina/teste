package br.com.tagme.gwt.commons.client;

import br.com.tagme.gwt.mvp.client.activity.DefaultActivity;

public class DefaultCompositeWithPresenter extends AbstractSankhyaPlaceComposite{
	
	public DefaultCompositeWithPresenter(){
		super();
	}
	
	public DefaultCompositeWithPresenter(DefaultActivity presenter){
		super(presenter);
	}
	
	public void setPresenter(DefaultActivity presenter){
		super.setActivity(presenter);
	}
	
	public DefaultActivity getPresenter(){
		return super.getActivity();
	}
}